{
  "name": "windtoday-app",
  "description": "Intelligent water sport marketplace",
  "homepage": "https://windtoday.co",
  "version": "3.0.0",
  "main": "index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windtoday/windtoday-app.git"
  },
  "bugs": {
    "url": "git+https://github.com/windtoday/windtoday-app.git"
  },
  "keywords": [
    "ecommerce",
    "marketplace",
    "windsurf",
    "windsurfing"
  ],
  "dependencies": {
    "algoliasearch": "~3.25.1",
    "autoprefixer": "~8.1.0",
    "babel-plugin-module-resolver": "~3.1.0",
    "babel-plugin-styled-components": "~1.5.0",
    "babel-plugin-transform-define": "~1.3.0",
    "babel-plugin-wrap-in-js": "~1.1.1",
    "breakpoint-sass": "~2.7.1",
    "colour-me-life": "~2.0.0",
    "config": "~1.30.0",
    "cssnano": "~3.10.0",
    "express": "~4.16.2",
    "glob": "~7.1.2",
    "js-yaml": "~3.11.0",
    "next": "~5.1.0",
    "node-sass": "~4.7.1",
    "normalize.css": "~8.0.0",
    "nprogress": "~0.2.0",
    "palx": "~1.0.2",
    "postcss-easy-import": "~3.0.0",
    "postcss-focus": "~3.0.0",
    "postcss-loader": "~2.1.0",
    "progress-arc-component": "~2.6.0",
    "prop-types": "~15.6.0",
    "qs": "~6.5.0",
    "raw-loader": "~0.5.1",
    "react": "~16.2.0",
    "react-dom": "~16.2.0",
    "react-feather": "~1.0.7",
    "react-headroom": "~2.2.2",
    "react-infinite-scroll-component": "~3.0.2",
    "react-instantsearch": "~5.0.1",
    "react-progress-bar.js": "~0.2.0-dev",
    "react-timeago": "~3.4.3",
    "rebass": "~2.0.0-0",
    "sass-loader": "~6.0.6",
    "styled-components": "~3.2.3",
    "styled-system": "2"
  },
  "devDependencies": {
    "babel-eslint": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "stylelint": "latest",
    "stylelint-config-standard": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "components",
    "config",
    "env.config.js",
    "next.config.js",
    "pages",
    "static",
    "util"
  ],
  "scripts": {
    "build": "NODE_ENV=production next build",
    "clean": "rm -rf node_modules && rm -rf .next",
    "dev": "next",
    "lint": "standard-markdown && standard",
    "precommit": "lint-staged",
    "predev": "rm -rf node_modules/.cache/babel-loader",
    "prestart": "npm run build",
    "pretty": "prettier-standard --single-quote {util,pages,config,components}/**/*.js",
    "start": "NODE_ENV=production next start -p $PORT",
    "test": "exit 0"
  },
  "private": "true",
  "license": "MIT",
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.js": [
      "prettier-standard",
      "git add"
    ]
  },
  "standard": {
    "globals": [
      "React"
    ],
    "parser": "babel-eslint"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "at-rule-no-unknown": [
        {
          "ignoreAtRules": [
            "include"
          ]
        },
        true
      ]
    }
  }
}
